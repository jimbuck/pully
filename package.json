{
  "name": "pully",
  "version": "2.1.2",
  "description": "A simple CLI for downloading high quality YouTube videos!",
  "main": "index.js",
  "bin": {
    "pully": "./dist/bin/pully.js"
  },
  "scripts": {
    "clean": "gulp clean",
    "rebuild": "gulp build",
    "watch": "gulp watch",
    "test": "gulp test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "preversion": "npm test",
    "version": "git add -A",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "url": "git+https://github.com/JimmyBoh/pully.git",
    "type": "git"
  },
  "author": "Jim Buck <jim@jimmyboh.com>",
  "contributors": [
    "Eirik Albrigtsen <analsandblaster@gmail.com>",
    "Mario Tacke <https://mariotacke.io/>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JimmyBoh/pully/issues"
  },
  "homepage": "https://github.com/JimmyBoh/pully#readme",
  "devDependencies": {
    "@types/node": "^6.0.46",
    "ava": "^0.16.0",
    "coveralls": "^2.11.15",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-shell": "^0.5.2",
    "nyc": "^8.4.0",
    "typescript": "^2.0.9",
    "yargs": "^6.4.0"
  },
  "dependencies": {
    "@types/through": "0.0.28",
    "@types/tmp": "0.0.31",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "conf": "^0.11.2",
    "fluent-ffmpeg": "^2.1.0",
    "lodash.template": "^4.4.0",
    "lodash.throttle": "^4.1.1",
    "log-update": "^1.0.2",
    "mkdirp-promise": "^4.0.1",
    "sanitize-filename": "^1.6.1",
    "tmp": "0.0.30",
    "universal-analytics": "^0.4.8",
    "update-notifier": "^1.0.2",
    "ytdl-core": "^0.7.19"
  },
  "ava": {
    "files": [
      "./dist/**/*.spec.js"
    ],
    "source": [
      "./dist/**/*.js"
    ]
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "exclude": [
      "dist/**/*.spec.js",
      "dist/bin/pully.js",
      "dist/example.js"
    ]
  }
}
